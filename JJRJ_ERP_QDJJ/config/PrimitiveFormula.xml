    <Parameter>[a],[h]</Parameter>
  </PrimitiveFormula>
  <PrimitiveFormula>
    <ImageIndex>2</ImageIndex>
    <TypeName>面积公式</TypeName>
    <FileName>2任意三角形面积</FileName>
    <Formula>sqrt(([a]+[b]+[c])*([b]+[c]-[a])*([a]+[c]-[b])*([a]+[b]-[c]))/4</Formula>
    <Parameter>[a],[b],[c]</Parameter>
  </PrimitiveFormula>
  <PrimitiveFormula>
    <ImageIndex>3</ImageIndex>
    <TypeName>面积公式</TypeName>
    <FileName>3平行四边形面积</FileName>
