    <Parameter>[a],[b],[h]</Parameter>
  </PrimitiveFormula>
  <PrimitiveFormula>
    <ImageIndex>5</ImageIndex>
    <TypeName>面积公式</TypeName>
    <FileName>5任意四边形面积</FileName>
    <Formula>sqrt((([a]+[b]+[c]+[d])/2-[a])*(([a]+[b]+[c]+[d])/2-[b])*(([a]+[b]+[c]+[d])/2-[c])*(([a]+[b]+[c]+[d])/2-[d]))</Formula>
    <Parameter>[a],[b],[c],[d]</Parameter>
  </PrimitiveFormula>
