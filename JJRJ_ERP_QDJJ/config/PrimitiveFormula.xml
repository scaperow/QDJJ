    <Parameter>[a],[b],[c],[h1],[h2]</Parameter>
  </PrimitiveFormula>
  <PrimitiveFormula>
    <ImageIndex>7</ImageIndex>
    <TypeName>面积公式</TypeName>
    <FileName>7缺圆环面积</FileName>
    <Formula>3.1415926*[θ]*([r2]*[r2]-[r1]*[r1])/360</Formula>
    <Parameter>[r1],[r2],[θ]</Parameter>
  </PrimitiveFormula>
